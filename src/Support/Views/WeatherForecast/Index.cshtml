@{
    ViewData["Title"] = "Weather Forecast";
}

<h2>Weather Forecast raw data</h2>

<div>@Html.Raw(ViewBag.JsonData)</div>



@using Microsoft.AspNetCore.Authentication

<h2>Logged in User Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>
